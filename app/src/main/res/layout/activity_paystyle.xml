<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <android.support.v7.widget.Toolbar
        android:id="@+id/logi_tool_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tool_bar_height"
        android:background="@color/white"
        app:navigationIcon="@mipmap/back_arrow_blue"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:text="计费方式"
            android:textColor="@color/c_000000"
            android:textSize="20sp"
            android:textStyle="normal" />
    </android.support.v7.widget.Toolbar>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="46px"
        android:layout_marginTop="30px"
        android:layout_marginRight="46px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="17px"
            android:layout_height="17px"
            android:layout_marginTop="25px"
            android:background="@drawable/bg_tv_circle" />

        <TextView
            android:id="@+id/tv_pay_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="20px"
            android:text=""
            android:textColor="@color/c_9b9b9b"
            android:textSize="35px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="46px"
        android:layout_marginRight="46px"
        android:layout_marginTop="26px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="17px"
            android:layout_height="17px"
            android:layout_marginTop="25px"
            android:background="@drawable/bg_tv_circle" />

        <TextView
            android:id="@+id/tv_pay_second"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="20px"
            android:text=""
            android:textColor="@color/c_9b9b9b"
            android:textSize="35px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="46px"
        android:layout_marginRight="46px"
        android:layout_marginTop="26px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="17px"
            android:layout_height="17px"
            android:layout_marginTop="25px"
            android:background="@drawable/bg_tv_circle" />

        <TextView
            android:id="@+id/tv_pay_three"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="20px"
            android:text=""
            android:textColor="@color/c_9b9b9b"
            android:textSize="35px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="46px"
        android:layout_marginRight="46px"
        android:layout_marginTop="26px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="17px"
            android:layout_height="17px"
            android:layout_marginTop="20px"
            android:background="@drawable/bg_tv_circle" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="20px"
            android:text="保费 = 实际发生运费 * 费率"
            android:textColor="@color/c_9b9b9b"
            android:textSize="35px" />
    </com.zhy.autolayout.AutoLinearLayout>

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="46px"
        android:layout_marginRight="46px"
        android:layout_marginTop="26px"
        android:orientation="horizontal">

        <TextView
            android:layout_width="17px"
            android:layout_height="17px"
            android:layout_marginTop="20px"
            android:background="@drawable/bg_tv_circle" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20px"
            android:layout_marginLeft="20px"
            android:text="总价 = 总运费 + 服务费 + 保费"
            android:textColor="@color/c_9b9b9b"
            android:textSize="35px" />
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>